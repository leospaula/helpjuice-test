<p id="notice"><%= notice %></p>

<h1>Articles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">Title</th>
      <th colspan="3">Body</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr><%= article.title %></tr>
      <tr><%= article.body %></tr>
    <% end %>
  </tbody>
</table>

<br>
